{
  "name": "jsx-ast-helpers",
  "version": "0.0.1",
  "description": "A set of helper functions for processing JSX AST nodes with Babel and TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "babel src --out-dir dist --extensions '.ts,.tsx'",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/jsx-helpers.git"
  },
  "keywords": [
    "jsx",
    "babel",
    "typescript",
    "ast",
    "helper"
  ],
  "author": "Your Name",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourusername/jsx-helpers/issues"
  },
  "homepage": "https://github.com/yourusername/jsx-helpers#readme",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.21.0",
    "@babel/preset-typescript": "^7.21.0",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@babel/types": "^7.21.0"
  }
}
